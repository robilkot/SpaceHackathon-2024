<?xml version="1.0" encoding="utf-8" ?>
<ContentPage 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="SpaceHackathon_2024.Views.NewsPage"         
    xmlns:extensions="clr-namespace:SpaceHackathon_2024.Helpers.Extensions"
    Title="News">
    
    <ContentPage.Resources>
        <ResourceDictionary>
            <Style 
                x:Key="TicketTitle"
                TargetType="Label">
                <Setter
                    Property="FontSize"
                    Value="18"/>
            </Style>
            <Style 
                x:Key="TicketDetail"
                TargetType="Label">
                <Setter
                    Property="FontSize"
                    Value="14"/>
            </Style>
            <DataTemplate x:Key="NewsTile">
                
            </DataTemplate>
        </ResourceDictionary>
    </ContentPage.Resources>

    <ScrollView>
        
    </ScrollView>
</ContentPage>